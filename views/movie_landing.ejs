  <script>
    function signin() {
      document.getElementById("myForm12").style.display = "block"; 
      var myvar = "<%=String(userID)%>";
      
      if (myvar==="undefined") {
   

        document.getElementById("profileid").style.display = "none";
        document.getElementById("logoutid").style.display = "none";
      }
      else {
        
        document.getElementById("profileid").style.display = "block";
        document.getElementById("logoutid").style.display = "block";
        document.getElementById("signinid").style.display = "none";
        document.getElementById("signupid").style.display = "none";

      }
    }

    function signincheck(){
      var username=String(document.getElementById('user').value);
      var password=String(document.getElementById('pass').value);
      
      var usertype=document.getElementById('usertype').checked;
      var eventtype=document.getElementById('eventtype').checked;
      
    var creds = {
      username: username,
      password: password,
      
    }
    var request = {
      method: 'POST',
      headers: {
        'Content-Type': 'application/json'
      },
      body: JSON.stringify(creds),
    }
    console.log(request);
    fetch(`/loginCheck`, request)
    .then(res => res.json())
    .then(user => {
      if (user._id && eventtype) {
        console.log(user._id);
       location.assign('/events?id=' + user._id)
     
      } else {
        document.getElementById('warn').innerHTML= 'Invalid credentials';
      }
    })
    .catch(err => {
      document.getElementById('warn').innerHTML= err;
      console.log('Error is', err)
    })

    }
  


