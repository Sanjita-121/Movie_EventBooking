    function openForm1(a) {

      var moviename, des, l, r;
       <%for (var i = 0; i < mov.length; i++) {%>
       if (a ==<%= i %>) {
          moviename = "<%= String(mov[i].mvname) %>";
          des = "<%= String(mov[i].mv_desc) %>";
          l = "<%= String(mov[i].mv_lang) %>";
          r = "<%= String(mov[i].mv_rating) %>";
        }
       <%}%>



      document.getElementById("myForm").style.display = "block";
      document.getElementById("mvname").innerHTML = String(moviename);
      document.getElementById("desc").innerHTML = String(des);
      document.getElementById("lang").innerHTML = String(l);
      document.getElementById("rating").innerHTML = String(r);


    }

    function closeForm() {
      document.getElementById("myForm").style.display = "none";
    }
